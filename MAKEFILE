clean_release: 
	cmd /c del /f $(OBJ_RELEASE) $(OUT_RELEASE)
	cmd /c rd bin\\Release
	cmd /c rd $(OBJDIR_RELEASE)\\src\\ACGCross
	cmd /c rd $(OBJDIR_RELEASE)\\src\\ACGCross\\GalgameActivity
	cmd /c rd $(OBJDIR_RELEASE)\\src\\ACGCross\\DanmakuGame
	cmd /c rd $(OBJDIR_RELEASE)\\src
	cmd /c rd $(OBJDIR_RELEASE)\\src\\SMLanguage
	cmd /c rd $(OBJDIR_RELEASE)\\src\\Core
	cmd /c rd $(OBJDIR_RELEASE)\\src\\ACGCross\\Title
	cmd /c rd $(OBJDIR_RELEASE)

.PHONY: before_build after_build before_debug after_debug clean_debug before_release after_release clean_release

